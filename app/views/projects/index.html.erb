<h1>Project: index</h1>
<p>Here are all the projects:</p>
<table>
   <tbody>
       <tr> 
           <th>Project</th>
           <th>Company</th>
       </tr>
  <% @projects.each do |project| %>
       <tr> 
           <th><%= link_to(project.name, project) %></th>
           <th><%= link_to(project.company.name, project.company) %></th>
       </tr>
  <% end %>
   </tbody>
</table>




