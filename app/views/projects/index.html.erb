<h1>Project: index</h1>
<p>Here are all the projects:</p>
<table>
   <tbody>
       <tr> 
           <th>Project</th>
           <th>Company</th>
       </tr>
  <% @projects.each do |project| %>
       <tr> 
           <td><%= link_to(project, project) %></td>
           <td><%= link_to(project.company, project.company) %></td>
       </tr>
  <% end %>
   </tbody>
</table>

<% content_for :aside do %>
  <% render 'sidebar' %>
<% end %>




